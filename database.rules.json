{
  "rules": {
    "map": {
      ".read": true,
      ".write": "auth !== null && auth.token.roles['map_editor'] === true && auth.token.auth_time > root.child('user_data').child(auth.uid).child('revokeTime').val()"
    },
    "node_content": {
      ".read": true,
      ".write": "auth !== null && auth.token.roles['map_editor'] === true && auth.token.auth_time > root.child('user_data').child(auth.uid).child('revokeTime').val()"
    },
    "node_image": {
      ".read": true,
      ".write": "auth !== null && auth.token.roles['map_editor'] === true && auth.token.auth_time > root.child('user_data').child(auth.uid).child('revokeTime').val()"
    },
    "precondition": {
      ".read": true,
      ".write": "auth !== null && auth.token.roles['map_editor'] === true && auth.token.auth_time > root.child('user_data').child(auth.uid).child('revokeTime').val()"
    },
    "trash": {
      "$nodeID": {
        ".read": true,
        ".write": "newData.exists() && !data.exists() && (auth !== null && auth.token.roles['map_editor'] === true && auth.token.auth_time > root.child('user_data').child(auth.uid).child('revokeTime').val())"
      }
    },
    "user_data": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": false,
        "comment": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        },
        "pins": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        }
      }
    },
    "public_user_data": {
      "$uid": {
        ".read": true,
        ".write": "$uid === auth.uid"
      }
    }
  }
}
